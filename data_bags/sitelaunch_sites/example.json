{
  // General info.
  "id": "example", // Required field for Chef data bags. This parameter is unique so it used to generate any site-related file or anything (site-specific) else.
  "sitename": "example", // Common variable. Used for info.
  "site_url": "loc.example.org", // Website URL.
  "drush_alias": "example.loc", // Drush alias. Do not include "@" symbol.
  // Directories.
  "sites_dir": "/var/www/", // Directory where websites installed.
  "project_dir": "example/", // Directory relative to "sites_dir" where project is.
  "drupal_dir": "docroot/", // Directory relative to "project_dir" where Drupal installed.
  "website_dir": "sites/example/", // Directory relative to "project_dir" (usually "sites/all/" if no multisiting.
  // Database info.
  "database": {
    "name": "example", // Database name. Ensure it is unique per data bags sitelaunch_sites.
    "user": "root",
    "password": "root"
  },
  // Acquia info.
  "acquia": {
    "user": "<>", // Your email.
    "password": "<>", // Your Cloud API private key. More details about credentials https://docs.acquia.com/cloud/api/auth
    "realm": "prod", // Realm. Usually prod. That doesn't mean that you break prod - you still will choose ENV.
    "site": "<>", // Name of site on Acquia.
    "db_name": "<>", // Usually db name is the same as "site". You don't need "raw" DB name here.
    "env": "dev" // ENV to use by default. TODO: --env option for provision?
  }
}